{
    "version": "2.0.0",
    "tasks": [
        {
            "taskName": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Sabs.AccountService/Sabs.AccountService.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "identifier": "publish",
            "label": "publish as netcoreapp",
            "type": "process",
            "command": "dotnet",
            "args": [
                "publish",
                "${workspaceFolder}/Sabs.AccountService/Sabs.AccountService.csproj",
                "-c",
                "Release"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}